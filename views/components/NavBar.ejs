<div class="navbar">
    <div class="container data-i18n">
        <p class="head">
            <%= lang.NavTitle %>
        </p>

        <div class="navitems" id="navitems">
        <!-- get all the NavItems -->
        <% for (let i=0; i < Object.keys(lang.NavItems).length; i++) { %>
            <!-- add onclick event redirect and add class active if its on the same oa -->
            <p class="item navbar-link" onclick="<%= lang.NavItems[i].link %>">
                <%= lang.NavItems[i].title %>
            </p>
            <% } %>
        </div>
        <i class="fa fa-bars navtogglebar" id="navtoggle" aria-hidden="true" onclick="toggleNavbar()"></i>
    </div>
</div>